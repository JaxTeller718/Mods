<configs>
	<config name="blocks">	
		<!-- Filing Cabinet By JaxTeller718 -->
		<append xpath="/blocks">	
			<block id="" name="filingCabinetNew">
				<property name="Extends" value="cntFileCabinet" />
		    	<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#filingCabinetNew?filingCabinetNewPrefab" />
        		<property name="CustomIcon" value="filingCabinetNew" />
        		<property name="DescriptionKey" value="filingCabinetNewDesc"/>
				<property name="Stacknumber" value="50"/>
            	<property name="Place" value="TowardsPlacerInverted"/>
				<property name="MultiBlockDim" value="1,2,1" />
			</block>
		</append>
	</config>

<config name="recipes">
<append xpath="/recipes">
	<!-- Filing Cabinet by JaxTeller718 -->
<recipe name="filingCabinetNew" count="1">
	<ingredient name="spring" count="4" />
	<ingredient name="forgedIron" count="15"/>
</recipe>
	<!-- Filing Cabinet -->
</append>
</config>


</configs>